{
  "name": "innate",
  "version": "0.0.1",
  "module": "index.ts",
  "type": "module",
  "prisma": {
    "schema": "./schema.prisma"
  },
  "devDependencies": {
    "bun-types": "^1.0.18",
    "commander": "^11.1.0",
    "concurrently": "^8.2.2",
    "typegraphql-prisma": "^0.27.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "dev": "dotenv -e .env bun src/scripts/dev.ts",
    "introspect-db": "dotenv -e ./.env ./node_modules/.bin/prisma db pull --schema=./schema.prisma",
    "prisma:generate": "dotenv -e ./.env prisma generate",
    "init-db": "bun src/scripts/initDb.ts",
    "create-db": "bun src/scripts/createDb.ts",
    "test": "dotenv -e .env -- bun test --preload ./setup-tests.ts",
    "test:debug": "DEBUG=1 dotenv -e .env -- bun test --preload ./setup-tests.ts",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@prisma/client": "5.7.0",
    "@types/graphql-fields": "^1.3.9",
    "chalk": "^5.3.0",
    "core-js": "^3.34.0",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "graphql": "15.7.2",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.4",
    "graphql-yoga": "^5.0.2",
    "patch-package": "^8.0.0",
    "postgres": "^3.4.3",
    "postinstall-postinstall": "^2.1.0",
    "prisma": "^5.7.0",
    "reflect-metadata": "^0.2.1",
    "strip-ansi": "^7.1.0",
    "tslib": "^2.6.2",
    "type-graphql": "^1.1.1"
  }
}
